<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <TextView
            android:layout_width="385dp"
            android:layout_height="82dp"
            android:text="회원관리"
            android:textSize="30sp"
            android:gravity="center"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.064"
            app:layout_constraintHorizontal_bias="0.502" android:id="@+id/textView"/>
    <Button
            android:text="회원추가"
            android:layout_width="259dp"
            android:layout_height="90dp" android:id="@+id/insertBtn"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintVertical_bias="0.073"/>
    <Button
            android:text="회원삭제"
            android:layout_width="259dp"
            android:layout_height="90dp" android:id="@+id/button2"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintVertical_bias="0.252"
            app:layout_constraintHorizontal_bias="0.501"/>
    <Button
            android:text="Button"
            android:layout_width="259dp"
            android:layout_height="90dp" android:id="@+id/button3"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintVertical_bias="0.441"
            app:layout_constraintHorizontal_bias="0.501"/>

</androidx.constraintlayout.widget.ConstraintLayout>