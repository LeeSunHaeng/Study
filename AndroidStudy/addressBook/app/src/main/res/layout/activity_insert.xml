<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".insertActivity">

    <TextView
            android:text="이름"
            android:layout_width="94dp"
            android:layout_height="30dp" android:id="@+id/textView"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="40dp"/>
    <TextView
            android:text="전화번호"
            android:layout_width="94dp"
            android:layout_height="30dp" android:id="@+id/textView6"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="676dp"/>
    <TextView
            android:text="나이"
            android:layout_width="94dp"
            android:layout_height="30dp" android:id="@+id/textView2"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="164dp"/>
    <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Relative"
            android:ems="10"
            android:id="@+id/relativeEditText"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toBottomOf="@+id/textView"
            android:layout_marginTop="268dp"/>
    <TextView
            android:text="주소"
            android:layout_width="94dp"
            android:layout_height="30dp" android:id="@+id/textView5"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="560dp"/>
    <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Job"
            android:ems="10"
            android:id="@+id/jobEditText"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toBottomOf="@+id/textView"
            android:layout_marginTop="400dp"/>
    <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Age"
            android:ems="10"
            android:id="@+id/ageEditText"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toBottomOf="@+id/textView"
            android:layout_marginTop="132dp"/>
    <TextView
            android:text="직업"
            android:layout_width="94dp"
            android:layout_height="30dp" android:id="@+id/textView4"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="432dp"/>
    <TextView
            android:text="관계"
            android:layout_width="94dp"
            android:layout_height="30dp" android:id="@+id/textView3"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="300dp"/>
    <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="PhoneNumber"
            android:ems="10"
            android:id="@+id/phoneNumberEditText"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toBottomOf="@+id/textView"
            android:layout_marginTop="644dp"/>
    <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Address"
            android:ems="10"
            android:id="@+id/addressEditText"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toBottomOf="@+id/textView"
            android:layout_marginTop="528dp"/>
    <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/nameEditText"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toBottomOf="@+id/textView"
            android:layout_marginTop="8dp"/>
    <Button
            android:text="추가"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/insertBtn"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="256dp" android:layout_marginBottom="4dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>