<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".PropileDetailActivity">
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:srcCompat="@tools:sample/avatars" android:id="@+id/img_profile"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="136dp" android:layout_marginStart="140dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="157dp"
            android:layout_height="44dp" android:id="@+id/user_name"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="312dp" android:layout_marginStart="124dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="151dp"
            android:layout_height="55dp" android:id="@+id/user_id"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="380dp" android:layout_marginStart="128dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="128dp"
            android:layout_height="55dp" android:id="@+id/user_pay"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="472dp" android:layout_marginStart="140dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>
