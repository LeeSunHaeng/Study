<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <TextView
            android:layout_width="469dp"
            android:layout_height="155dp"
            android:text="Hello World!"
            android:textSize="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.503"
            app:layout_constraintVertical_bias="0.834" android:id="@+id/textView"/>
    <TextView
            android:text="위도"
            android:textSize="20dp"
            android:layout_width="100dp"
            android:layout_height="50dp" android:id="@+id/latTextView"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="48dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="44dp"/>
    <TextView
            android:text="경도"
            android:layout_width="100dp"
            android:textSize="20dp"
            android:layout_height="50dp" android:id="@+id/lonTextView"
            app:layout_constraintTop_toBottomOf="@+id/latTextView" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="40dp" android:layout_marginStart="44dp"/>
    <EditText
            android:layout_width="361dp"
            android:layout_height="50dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/latText"
            app:layout_constraintStart_toEndOf="@+id/latTextView" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginStart="32dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.079" app:layout_constraintVertical_bias="0.052"/>
    <EditText
            android:layout_width="361dp"
            android:layout_height="50dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/lonText"
            app:layout_constraintStart_toEndOf="@+id/lonTextView" app:layout_constraintTop_toBottomOf="@+id/latText"
            android:layout_marginTop="40dp" android:layout_marginStart="32dp"/>
    <EditText
            android:layout_width="361dp"
            android:layout_height="50dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/addText"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="292dp" android:layout_marginStart="172dp"/>
    <TextView
            android:text="주소"
            android:layout_width="100dp"
            android:layout_height="50dp" android:id="@+id/addTextView"
            app:layout_constraintTop_toBottomOf="@+id/lonTextView" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="104dp" android:layout_marginStart="44dp"/>
    <Button
            android:text="주소로 변환"
            android:layout_width="159dp"
            android:layout_height="60dp" android:id="@+id/addrBtn"
            app:layout_constraintTop_toBottomOf="@+id/lonText" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="28dp" android:layout_marginStart="240dp"/>
    <Button
            android:text="지도"
            android:layout_width="104dp"
            android:layout_height="64dp" android:id="@+id/mapBtn1"
            app:layout_constraintStart_toEndOf="@+id/addrBtn" android:layout_marginTop="28dp"
            app:layout_constraintTop_toBottomOf="@+id/lonText" android:layout_marginStart="32dp"/>
    <Button
            android:text="지도"
            android:layout_width="114dp"
            android:layout_height="59dp" android:id="@+id/mapBtn2"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="392dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="428dp"/>
    <Button
            android:text="위도/경도로 변환"
            android:layout_width="153dp"
            android:layout_height="59dp" android:id="@+id/latBtn"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="392dp" android:layout_marginStart="240dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>